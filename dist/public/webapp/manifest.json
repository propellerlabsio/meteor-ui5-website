{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "webapp",
        "type": "application",
        "title": "UI5 Meteor Website",
        "description": "Docs, demos and tutorials for the Meteor UI5 package.",
        "applicationVersion": {
            "version": "0.0.1"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "webapp.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "": {
                "type": "meteor-ui5.model.mongo.Model"
            }
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"
            }]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "webapp.view",
                "controlId": "SplitApp",
                "clearTarget": false
            },
            "routes": [{
                "pattern": "",
                "name": "home",
                "target": ["master", "home"]
            }, {
                "pattern": "demos:?query:",
                "name": "demos",
                "target": ["masterDemos", "demosIntro"]
            }, {
                "pattern": "demo/{groupId}/{demoId}:?query:",
                "name": "demo",
                "target": ["masterDemos", "demo"]
            }, {
                "pattern": "tutorial",
                "name": "tutorial",
                "target": ["master", "tutorial"]
            }, {
                "pattern": "docs",
                "name": "docs",
                "target": ["masterDocs", "docs"]
            }, {
                "pattern": "docs/api",
                "name": "docsApi",
                "target": ["masterDocs", "docsApi"]
            }, {
                "pattern": "docs/quickstart",
                "name": "docsQuickstart",
                "target": ["masterDocs", "docsQuickstart"]
            }],
            "targets": {
                "master": {
                    "viewName": "Master",
                    "controlAggregation": "masterPages",
                    "viewLevel": "1"
                },
                "masterDemos": {
                    "viewName": "MasterDemos",
                    "controlAggregation": "masterPages",
                    "viewLevel": "2"
                },
                "masterDocs": {
                    "viewName": "MasterDocs",
                    "controlAggregation": "masterPages",
                    "viewLevel": "2"
                },
                "home": {
                    "viewName": "home/Home",
                    "controlAggregation": "detailPages",
                    "viewLevel": "1",
                    "transition": "flip"
                },
                "demosIntro": {
                    "viewName": "DemosIntro",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "docs": {
                  "viewName": "Docs",
                  "controlAggregation": "detailPages",
                  "viewLevel": "2",
                  "transition": "flip"
                },
                "docsApi": {
                  "viewName": "DocsApi",
                  "controlAggregation": "detailPages",
                  "viewLevel": "2",
                  "transition": "flip"
                },
                "docsQuickstart": {
                  "viewName": "DocsQuickstart",
                  "controlAggregation": "detailPages",
                  "viewLevel": "2",
                  "transition": "flip"
                },
                "demo": {
                    "viewName": "Demo",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "tutorial": {
                    "viewName": "Tutorial",
                    "controlAggregation": "detailPages",
                    "viewLevel": "1",
                    "transition": "flip"
                }
            }
        }
    }
}
