{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "meteor-model-demo",
        "type": "application",
        "title": "UI5 Meteor Model Demo",
        "description": "Demonstration of the UI5 Meteor Model.",
        "applicationVersion": {
            "version": "0.0.1"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "meteor-model-demo.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "meteor-model-demo.view",
                "controlId": "SplitApp",
                "clearTarget": false
            },
            "routes": [{
                "pattern": "",
                "name": "loading",
                "target": ["loading", "welcome"]
            },{
                "pattern": "home",
                "name": "home",
                "target": ["master", "welcome"]
            },{
                "pattern": "categories",
                "name": "categories",
                "target": ["masterCategories", "categoriesMeteor"]
            },{
                "pattern": "categoriesJson",
                "name": "categoriesJson",
                "target": ["masterCategories", "categoriesJson"]
            },{
                "pattern": "categoriesFlatJson",
                "name": "categoriesFlatJson",
                "target": ["masterCategories", "categoriesFlatJson"]
            },{
                "pattern": "categoriesOData",
                "name": "categoriesOData",
                "target": ["masterCategories", "categoriesOData"]
            }
          ],
            "targets": {
                "loading": {
                    "viewName": "Loading",
                    "controlAggregation": "masterPages",
                    "viewLevel": "1"
                },
                  "master": {
                    "viewName": "Master",
                    "controlAggregation": "masterPages",
                    "viewLevel": "1"
                },
                "masterCategories": {
                    "viewName": "MasterCategories",
                    "controlAggregation": "masterPages",
                    "viewLevel": "2"
                },
                "welcome": {
                    "viewName": "Welcome",
                    "controlAggregation": "detailPages",
                    "viewLevel": "1",
                    "transition": "flip"
                },
                "categoriesMeteor": {
                    "viewName": "CategoriesMeteor",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "categoriesJson": {
                    "viewName": "CategoriesJson",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "categoriesFlatJson": {
                    "viewName": "CategoriesFlatJson",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "categoriesOData": {
                    "viewName": "CategoriesOData",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                }
            }
        }
    }
}
