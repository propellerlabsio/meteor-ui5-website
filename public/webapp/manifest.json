{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "meteor-ui5-demo",
        "type": "application",
        "title": "UI5 Meteor Model Demo",
        "description": "Demonstration of the UI5 Meteor Model.",
        "applicationVersion": {
            "version": "0.0.1"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "meteor-ui5-demo.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "": {
                "type": "meteor-ui5.MeteorMongoModel"
            }
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"
            }]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "meteor-ui5-demo.view",
                "controlId": "SplitApp",
                "clearTarget": false
            },
            "routes": [{
                "pattern": "",
                "name": "home",
                "target": ["master", "home"]
            }, {
                "pattern": "tutorial",
                "name": "tutorial",
                "target": ["master", "tutorial"]
            }, {
                "pattern": "binding",
                "name": "binding",
                "target": ["masterBinding", "binding"]
            }, {
                "pattern": "binding/examples",
                "name": "bindingExamples",
                "target": ["masterBinding", "bindingExamples"]
            }, {
                "pattern": "binding/examples/{id}",
                "name": "bindingExample",
                "target": ["masterBinding", "bindingExample"]
            }, {
                "pattern": "compare/meteor:?query:",
                "name": "compareMeteor",
                "target": ["masterCompare", "compareMeteor"]
            }, {
                "pattern": "compare/json:?query:",
                "name": "compareJson",
                "target": ["masterCompare", "compareJson"]
            }, {
                "pattern": "compare/odata:?query:",
                "name": "compareOData",
                "target": ["masterCompare", "compareOData"]
            }],
            "targets": {
                "master": {
                    "viewName": "Master",
                    "controlAggregation": "masterPages",
                    "viewLevel": "1"
                },
                "masterCompare": {
                    "viewName": "MasterCompare",
                    "controlAggregation": "masterPages",
                    "viewLevel": "2"
                },
                "masterBinding": {
                    "viewName": "MasterBinding",
                    "controlAggregation": "masterPages",
                    "viewLevel": "3"
                },
                "home": {
                    "viewName": "Home",
                    "controlAggregation": "detailPages",
                    "viewLevel": "1",
                    "transition": "flip"
                },
                "tutorial": {
                    "viewName": "Tutorial",
                    "controlAggregation": "detailPages",
                    "viewLevel": "1",
                    "transition": "flip"
                },
                "binding": {
                    "viewName": "Binding",
                    "controlAggregation": "detailPages",
                    "viewLevel": "3",
                    "transition": "flip"
                },
                "bindingExamples": {
                    "viewName": "BindingExamples",
                    "controlAggregation": "detailPages",
                    "viewLevel": "3",
                    "transition": "flip"
                },
                "bindingExample": {
                    "viewName": "BindingExample",
                    "controlAggregation": "detailPages",
                    "viewLevel": "4"
                },
                "compareMeteor": {
                    "viewName": "CompareMeteor",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "compareJson": {
                    "viewName": "CompareJson",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "compareOData": {
                    "viewName": "CompareOData",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "viewSourceCode": {
                    "viewName": "ViewSourceCode",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                }
            }
        }
    }
}
