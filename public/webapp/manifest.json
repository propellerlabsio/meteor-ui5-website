{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "meteor-model-demo",
        "type": "application",
        "title": "UI5 Meteor Model Demo",
        "description": "Demonstration of the UI5 Meteor Model.",
        "applicationVersion": {
            "version": "0.0.1"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "meteor-model-demo.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "meteor-model-demo.view",
                "controlId": "SplitApp",
                "clearTarget": false
            },
            "routes": [{
                "pattern": "",
                "name": "home",
                "target": ["master", "welcome"]
            },{
                "pattern": "carousel",
                "name": "carousel",
                "target": ["masterCarousel", "carouselMeteor"]
            },{
                "pattern": "carouselJson",
                "name": "carouselJson",
                "target": ["masterCarousel", "carouselJson"]
            }
          ],
            "targets": {
                "master": {
                    "viewName": "Master",
                    "controlAggregation": "masterPages",
                    "viewLevel": "1"
                },
                "masterCarousel": {
                    "viewName": "MasterCarousel",
                    "controlAggregation": "masterPages",
                    "viewLevel": "2"
                },
                "welcome": {
                    "viewName": "Welcome",
                    "controlAggregation": "detailPages",
                    "viewLevel": "1",
                    "transition": "flip"
                },
                "carouselMeteor": {
                    "viewName": "CarouselMeteor",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "carouselJson": {
                    "viewName": "CarouselJson",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                }
            }
        }
    }
}
