{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "meteor-ui5-demo",
        "type": "application",
        "title": "UI5 Meteor Model Demo",
        "description": "Demonstration of the UI5 Meteor Model.",
        "applicationVersion": {
            "version": "0.0.1"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "meteor-ui5-demo.view.App",
        "models": {
            "": {
                "type": "meteor-ui5-demo.model.meteor.mongo.MeteorMongoModel",
                "settings": {}
            }
        },
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "meteor-ui5-demo.view",
                "controlId": "SplitApp",
                "clearTarget": false
            },
            "routes": [{
                "pattern": "",
                "name": "home",
                "target": ["master", "home"]
            },{
                "pattern": "compareMeteor",
                "name": "compareMeteor",
                "target": ["masterCompare", "categoriesMeteor"]
            },{
                "pattern": "compareJson",
                "name": "compareJson",
                "target": ["masterCompare", "categoriesJson"]
            },{
                "pattern": "compareFlatJson",
                "name": "compareFlatJson",
                "target": ["masterCompare", "categoriesFlatJson"]
            },{
                "pattern": "compareOData",
                "name": "compareOData",
                "target": ["masterCompare", "categoriesOData"]
            }
          ],
            "targets": {
                "loading": {
                    "viewName": "Loading",
                    "controlAggregation": "masterPages",
                    "viewLevel": "1"
                },
                  "master": {
                    "viewName": "Master",
                    "controlAggregation": "masterPages",
                    "viewLevel": "1"
                },
                "masterCompare": {
                    "viewName": "MasterCompare",
                    "controlAggregation": "masterPages",
                    "viewLevel": "2"
                },
                "home": {
                    "viewName": "Home",
                    "controlAggregation": "detailPages",
                    "viewLevel": "1",
                    "transition": "flip"
                },
                "categoriesMeteor": {
                    "viewName": "CategoriesMeteor",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "categoriesJson": {
                    "viewName": "CategoriesJson",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "categoriesFlatJson": {
                    "viewName": "CategoriesFlatJson",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                },
                "categoriesOData": {
                    "viewName": "CategoriesOData",
                    "controlAggregation": "detailPages",
                    "viewLevel": "2",
                    "transition": "flip"
                }
            }
        }
    }
}
