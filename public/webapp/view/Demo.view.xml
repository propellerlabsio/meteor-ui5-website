<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns:website="website.control"
    xmlns="sap.m"
    controllerName="website.controller.Demo">

  <Page id="compareModel"
      binding="{viewState>/Demo}"
      title="{viewState>title}"
      class="sapUiStdPage">
    <content>

      <website:DemoViewer groupId="{viewState>groupId}"
          demoId="{viewState>_id}"
          infoFile="{viewState>infoFile}"
          demoViewName="{viewState>demoViewName}" />
    </content>
  </Page>
</mvc:View>
