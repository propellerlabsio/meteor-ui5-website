<mvc:View
    	xmlns:mvc="sap.ui.core.mvc"
      xmlns:meteor-ui5-demo="meteor-ui5-demo.control"
      xmlns="sap.m"
      controllerName="meteor-ui5-demo.controller.CompareModels">

		<Page id="compareJsonPage" title="JSON Model" class="sapUiStdPage">
 		  <content>
        <IconTabBar
          id="tabsCompare"
          select="onTabSelect"
          expanded="true"
          expandable="false"
          class="sapUiResponsiveContentPadding">
          <items>
            <IconTabFilter
              key="demo"
              text="Demo">
              <mvc:XMLView viewName="meteor-ui5-demo.view.CategoriesJson"></mvc:XMLView>
            </IconTabFilter>
            <IconTabFilter
              key="view"
              text="CategoriesJson.view.xml">
              <meteor-ui5-demo:SourceCodeViewer
                id="sourceJsonView"
                src='webapp/view/CategoriesJson.view.xml'
                hljsLanguage='xml'/>
            </IconTabFilter>
            <IconTabFilter
              key="controller"
              text="CategoriesJson.controller.js">
              <meteor-ui5-demo:SourceCodeViewer
                id="sourceJsonController"
                src='webapp/controller/CategoriesJson-dbg.controller.js'
                hljsLanguage='js'/>
            </IconTabFilter>
            <IconTabFilter
              key="table"
              text="CategoriesTable.fragment.xml">
              <meteor-ui5-demo:SourceCodeViewer
                id="sourceJsonTable"
                src='webapp/view/CategoriesTable.fragment.xml'
                hljsLanguage='xml'/>
            </IconTabFilter>
          </items>
        </IconTabBar>
			</content>
		</Page>
</mvc:View>
