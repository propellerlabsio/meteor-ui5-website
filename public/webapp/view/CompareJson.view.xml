<mvc:View
    	xmlns:mvc="sap.ui.core.mvc"
      xmlns:meteor-ui5-demo="meteor-ui5-demo.control"
      xmlns="sap.m">
      <!-- controllerName="meteor-ui5-demo.controller.CompareModels"> -->

		<Page title="JSON Model" class="sapUiStdPage">
 		  <content>
        <IconTabBar
          expanded="true"
          expandable="false"
          class="sapUiResponsiveContentPadding">
          <items>
            <IconTabFilter text="Demo">
              <mvc:XMLView viewName="meteor-ui5-demo.view.CategoriesJson"></mvc:XMLView>
            </IconTabFilter>
            <IconTabFilter text="CategoriesJson.view.xml">
              <meteor-ui5-demo:SourceCodeViewer
                src='webapp/view/CategoriesJson.view.xml'
                hljsLanguage='xml'/>
            </IconTabFilter>
            <IconTabFilter text="CategoriesJson.controller.js">
              <meteor-ui5-demo:SourceCodeViewer
                src='webapp/controller/CategoriesJson-dbg.controller.js'
                hljsLanguage='js'/>
            </IconTabFilter>
            <IconTabFilter text="CategoriesTable.fragment.xml">
              <meteor-ui5-demo:SourceCodeViewer
                src='webapp/view/CategoriesTable.fragment.xml'
                hljsLanguage='xml'/>
            </IconTabFilter>
          </items>
        </IconTabBar>
			</content>
		</Page>
</mvc:View>
