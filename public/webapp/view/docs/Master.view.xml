<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns="sap.m"
    controllerName="meteor-ui5-website.controller.Master">
  <Page id="master"
      title="Docs"
      showNavButton="true"
      navButtonPress="onPressBack"
      icon="sap-icon://action"
      class="sapUiStdPage">
    <content>
      <List id="masterListDocs"
          itemPress="onItemPress"
          mode="SingleSelectMaster">
        <items>
          <StandardListItem id="quickstart"
              title="Quickstart Guide"
              type="Active"
              custom:to="docsQuickstart" />
          <StandardListItem id="api-docs"
              title="API docs"
              type="Active"
              custom:to="docsApi" />
        </items>
      </List>
    </content>
  </Page>
</mvc:View>
