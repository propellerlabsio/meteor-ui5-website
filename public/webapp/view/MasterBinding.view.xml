<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  xmlns="sap.m"
  controllerName="meteor-ui5-demo.controller.Master">
    <Page id="masterCompare" title="Binding Examples" icon="sap-icon://action" class="sapUiStdPage" showNavButton="true" navButtonPress="onPressBack">
        <content>
            <List id="masterList" itemPress="onItemPress" mode="SingleSelectMaster">
                <items>
                    <StandardListItem id="bindingAllOrders" title="All Orders" type="Active" custom:to="bindingAllOrders" />
                    <StandardListItem id="bindingSomeOrders" title="Some Orders" type="Active" custom:to="bindingSomeOrders" />
                    <StandardListItem id="bindingOneOrder" title="One Order" type="Active" custom:to="bindingOneOrder" />
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
