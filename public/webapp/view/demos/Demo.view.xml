<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns:meteor-ui5-website="meteor-ui5-website.control"
    xmlns="sap.m"
    controllerName="meteor-ui5-website.controller.demos.Demo">

  <Page id="compareModel"
      binding="{viewState>/Demo}"
      title="{viewState>title}"
      class="sapUiStdPage">
    <content>

      <meteor-ui5-website:DemoViewer groupId="{viewState>groupId}"
          demoId="{viewState>_id}"
          infoFile="{viewState>infoFile}"
          demoViewName="{viewState>demoViewName}" />
    </content>
  </Page>
</mvc:View>
