<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  xmlns="sap.m"
  controllerName="meteor-model-demo.controller.MasterCompare">
    <Page id="masterCompare" title="Compare Models" icon="sap-icon://action" class="sapUiStdPage" showNavButton="true" navButtonPress="onPressBack">
        <content>
            <List id="masterCompareList" itemPress="onItemPress" mode="SingleSelectMaster">
                <items>
                    <StandardListItem id="compareMeteor" title="MeteorModel" type="Active" custom:to="compareMeteor"/>
                    <StandardListItem id="compareJson" title="JSONModel" type="Active" custom:to="compareJson"/>
                    <StandardListItem id="compareOData" title="ODataModel" type="Active" custom:to="compareOData"/>
                    <StandardListItem id="compareFlatJson" title="Flat JSONModel (debugging)" type="Active" custom:to="compareFlatJson"/>
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
