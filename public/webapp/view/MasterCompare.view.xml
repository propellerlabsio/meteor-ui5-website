<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  xmlns="sap.m"
  controllerName="meteor-ui5.controller.Master">
    <Page id="masterCompare" title="Compare UI5 Models" icon="sap-icon://action" class="sapUiStdPage" showNavButton="true" navButtonPress="onPressBack">
        <content>
            <List id="masterList" itemPress="onItemPress" mode="SingleSelectMaster">
                <items>
                    <StandardListItem id="compareMeteor" title="Meteor" icon="/art/logos/meteor.png" type="Active" custom:to="compareMeteor" custom:preserveQueryParams="true"/>
                    <StandardListItem id="compareJson" title="JSON" icon="/art/logos/json.png" type="Active" custom:to="compareJson" custom:preserveQueryParams="true"/>
                    <StandardListItem id="compareOData" title="OData" icon="/art/logos/odata.png" type="Active" custom:to="compareOData" custom:preserveQueryParams="true"/>
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
