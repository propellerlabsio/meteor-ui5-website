<mvc:View
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  controllerName="website.demo.binding.one-order.OneOrder">

  <ObjectHeader title="Order {_id}"
      binding="{/Orders(10248)}"
      number="{= ${Items}.length}"
      numberUnit="Items">
    <attributes>
      <ObjectAttribute title="Customer"
          text="{CustomerID}" />
      <ObjectAttribute title="Country"
          text="{ShipCountry}" />
    </attributes>
  </ObjectHeader>

</mvc:View>
