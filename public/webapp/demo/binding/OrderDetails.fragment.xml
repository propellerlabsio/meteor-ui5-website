<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">

    <ObjectHeader
        title="Order {OrderID}"
        number="{= ${Items}.length}"
        numberUnit="Items">
        <attributes>
            <ObjectAttribute text="{CustomerID}" />
            <ObjectAttribute text="{ShipCity}" />
            <ObjectAttribute text="{ShipCountry}" />
        </attributes>
    </ObjectHeader>

</core:FragmentDefinition>
